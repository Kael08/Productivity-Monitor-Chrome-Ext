{
    "manifest_version": 3,
    "name": "FocusGuard",
    "version": "1.0",
    "permissions": ["tabs", "scripting", "webNavigation", "activeTab","storage","alarms"],
    "background": {
      "service_worker": "background.js",
      "persistent": false
    },
    "host_permissions": ["<all_urls>"]
}  